{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "no-unused-vars": 1,
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
